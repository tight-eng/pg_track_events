{
  "name": "docs",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "next build",
    "copy-readme": "cp ../README.md content/docs/index.mdx && echo \"---\\ntitle: 'Introduction'\\ndescription: 'Emit analytics events as your data changes'\\n---\\n$(cat content/docs/index.mdx)\" > content/docs/index.mdx",
    "dev": "npm run copy-readme && next dev --turbo",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "pages:build": "npm run copy-readme && npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy"
  },
  "dependencies": {
    "framer-motion": "^12.9.4",
    "fumadocs-core": "15.2.12",
    "fumadocs-mdx": "11.6.1",
    "fumadocs-ui": "15.2.12",
    "lucide-react": "^0.507.0",
    "next": "15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^8",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vercel": "^41.7.0",
    "wrangler": "^4.14.1"
  }
}
